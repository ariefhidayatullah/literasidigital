				var thumbnail = viewInstance.stage.toDataURL({multiplier: scaling, format: 'png'});

			product.push({
				title: viewInstance.title,
				thumbnail: thumbnail,
				elements: [],
				options: relevantViewOpts,
				names_numbers: viewInstance.names_numbers
