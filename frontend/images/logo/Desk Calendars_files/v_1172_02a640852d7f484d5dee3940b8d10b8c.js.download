		if(undo) {
			instance.undos.push(undo);

			if(instance.undos.length > 20) {
				instance.undos.shift();
			}
		}

		if(redo) {
			instance.redos.push(redo);
		}

		if(trigger) {
		
			$this.trigger('undoRedoSet', [instance.undos, instance.redos]);

		}

